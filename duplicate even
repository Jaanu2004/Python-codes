
n=int(input())
l=list(map(int,input().split()))
u=[]
d=[]
for i in l:#calling data directly
    if i%2==0 not in u:#2 not in u
        u.append(i)
    else:
        d.append(i)
for i in u:#printing unique numbers
    print(i,end=' ')
res=u+d#mergining two list
#n.extend(p)
for i in res:
    print(i,end=' ')

    
